		 <put-attribute name="{##TILES_LAYOUT_PREFIX##}Content"	value="{##PAGE_DIR##}/{##LASTDIR##}/{##BIZNM##}Write.jsp" />
	 </definition>

	 <definition name="{##ADDR_PREFIX##}/{##LASTDIR##}{##BIZNM_UP1##}/{##BIZNM##}View" extends="{##TILES_LAYOUT_CONT##}">
		 <put-attribute name="{##TILES_LAYOUT_PREFIX##}Title"	value="{##BIZKR##} 상세보기" />
		 <put-attribute name="{##TILES_LAYOUT_PREFIX##}Script"	value="{##SCRIPT_DIR##}/script_{##BIZNM##}.jsp"/>
		 <put-attribute name="{##TILES_LAYOUT_PREFIX##}Content"	value="{##PAGE_DIR##}/{##LASTDIR##}/{##BIZNM##}View.jsp" />
	 </definition>

