	 <definition name="{##ADDR_PREFIX##}/{##LASTDIR##}{##BIZNM_UP1##}/{##BIZNM##}List" extends="{##TILES_LAYOUT_CONT##}">
		 <put-attribute name="{##TILES_LAYOUT_PREFIX##}Title"	value="{##BIZKR##} 목록" />
		 <put-attribute name="{##TILES_LAYOUT_PREFIX##}Script"	value="{##SCRIPT_DIR##}/script_{##BIZNM##}.jsp"/>
		 <put-attribute name="{##TILES_LAYOUT_PREFIX##}Content"	value="{##PAGE_DIR##}/{##LASTDIR##}/{##BIZNM##}List.jsp" />
	 </definition>

	 <definition name="{##ADDR_PREFIX##}/{##LASTDIR##}{##BIZNM_UP1##}/{##BIZNM##}Update" extends="{##TILES_LAYOUT_EDIT##}">
